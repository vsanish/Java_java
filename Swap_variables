package org.newproject;
import java.util.*;

public class Swapping2noUsing3rdVariable {
	public static void main (String[] args) {
		int a,b,temp;
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the number a and b:");
		a=sc.nextInt();
		b=sc.nextInt();
		
		System.out.println("Before the Swapping:\n a= "+a+"\n b= "+b);
		
		temp = a;
		a=b;
		b=temp;
		
		System.out.println("After the Swapping:\n a= "+a+"\n b= "+b);
		
	}

}
